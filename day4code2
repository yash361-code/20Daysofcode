public static void main(String[] args)
{ 
System.out.println("Enter the no of elements  of array")
 int n = arr.length;
System.out.println("Enter the elements of array")
    int arr[] = new Int[n]
   

    System.out.print(notoccuring(arr, n));
}
class day4code2 {
	  
    static int notoccuring(int arr[], int n)
    {
        for (int i = 0; i < n; i++) {
            int j;
            for (j = 0; j < n; j++)
                if (i != j && arr[i] == arr[j])
                    break;
            if (j == n)
                return arr[i];
        }
  
        return -1;
    }
}
  
