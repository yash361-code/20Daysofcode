public static void main(String[] args)
{

    int arr[] = { 6, 1, 6, 6, 7, 1 };
    int n = arr.length;

    System.out.print(notoccuring(arr, n));
}
class day4code2 {
	  
    static int notoccuring(int arr[], int n)
    {
        for (int i = 0; i < n; i++) {
            int j;
            for (j = 0; j < n; j++)
                if (i != j && arr[i] == arr[j])
                    break;
            if (j == n)
                return arr[i];
        }
  
        return -1;
    }
}
  
